<datastore-indexes autoGenerate="true">

    <datastore-index kind="Contact" ancestor="false" source="auto">
        <property name="type" direction="asc"/>
        <property name="name" direction="asc"/>
    </datastore-index>

    <!-- Used 1 time in query history -->
    <datastore-index kind="Policy" ancestor="false" source="auto">
        <property name="issueDate" direction="asc"/>
        <property name="bookTime" direction="asc"/>
        <property name="regNum" direction="asc"/>
    </datastore-index>

    <!-- Used 1 time in query history -->
    <datastore-index kind="A7Form" ancestor="false" source="auto">
        <property name="owner_key_OID" direction="asc"/>
        <property name="status" direction="asc"/>
        <property name="regNum" direction="asc"/>
    </datastore-index>

</datastore-indexes>
